  <!-- interface-requires glade_widgets 0.0 -->
  <!-- interface-naming-policy project-wide -->
  <object class="GtkListStore" id="nicks_store">
    <columns>
      <!-- column-name Prefix -->
      <column type="gchararray"/>
      <!-- column-name Nick -->
      <column type="gchararray"/>
    </columns>
  </object>
  <object class="GtkTreeStore" id="tab_store">
    <columns>
