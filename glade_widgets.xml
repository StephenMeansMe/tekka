<?xml version="1.0" encoding="UTF-8"?>
<!-- Start glade with GLADE_CATALOG_PATH=. glade-3 -->
<glade-catalog name="glade_widgets" library="gladepython" domain="glade-3" depends="gtk+">

	<init-function>glade_python_init</init-function>

	<glade-widget-classes>
        <glade-widget-class title="Spell Entry" generic-name="spellentry"
							name="SpellEntry"/>

		<glade-widget-class title="Search Bar" generic-name="searchbar"
							name="SearchBar">
			<properties>
				<property id="n-rows" query="False" default="1"
						save="False" visible="False"/>
				<property id="n-columns" query="False" default="1"
						save="False" visible="False"/>
			</properties>
		</glade-widget-class>
		<glade-widget-class title="Output Shell" generic-name="outputshell"
							name="OutputShell">
			<properties>
				<property id="size" query="False" default="1" save="False"
						  visible="False"/>
			</properties>
		</glade-widget-class>
		<glade-widget-class title="Output Window"
							generic-name="outputwindow"
							name="OutputWindow">
		</glade-widget-class>
		<glade-widget-class title="HTML Buffer"
							generic-name="htmlbuffer"
							name="HTMLBuffer"/>
		<glade-widget-class title="GO HTML Buffer"
							generic-name="gohtmlbuffer"
							name="GOHTMLBuffer"/>
		<glade-widget-class title="Expanding List"
							generic-name="expandinglist"
							name="ExpandingList">
			<properties>
				<property id="n-rows" query="False" default="1"
						  save="False" visible="False"/>
				<property id="n-columns" query="False" default="1"
						  save="False" visible="False"/>
				<property id="str_widgets" query="True"
						  default="(gtk.Entry,)"
						  save="True" visible="True"
						  name="Widgets"/>
				<property id="no_first_row" query="True"
						  default="False"
						  save="True" visible="True"/>
			</properties>
		</glade-widget-class>
		<glade-widget-class title="Contrast Color Table"
							generic-name="contrastcolortable"
							name="ContrastColorTable">
			<properties>
				<property id="n-rows" query="False" default="1"
						  save="False" visible="False"/>
				<property id="n-columns" query="False" default="1"
						  save="False" visible="False"/>
				<property id="gcolumns" query="True" default="6"
						  save="True" visible="True" name="Columns"/>
			</properties>
		</glade-widget-class>
    </glade-widget-classes>

	<glade-widget-group name="TekkaWidgets" title="Tekka Widgets">
        <glade-widget-class-ref name="SpellEntry"/>
		<glade-widget-class-ref name="SearchBar"/>
		<glade-widget-class-ref name="OutputShell"/>
		<glade-widget-class-ref name="OutputWindow"/>
		<glade-widget-class-ref name="HTMLBuffer"/>
		<glade-widget-class-ref name="GOHTMLBuffer"/>
		<glade-widget-class-ref name="ExpandingList"/>
		<glade-widget-class-ref name="ContrastColorTable"/>
	</glade-widget-group>

</glade-catalog>
